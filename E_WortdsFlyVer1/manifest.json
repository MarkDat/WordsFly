{
    "name" : "WordsFly",
    "description" : "Đây là extension do Thành Đạt Làm  nhaa : )) !",
    "manifest_version": 2,
    "version":"0.1.12",
    "background":{
        "scripts": ["./br.js"],
        "persistent":false
    },

    "browser_action":{
        "default_icon":"./icon.png",
        "default_popup":"./index.html"
    },

    "permissions":["activeTab"],

    "content_scripts":[
        {
            "matches":["http://*/*", "https://*/*"],
            "js":["jquery.min.js","js.js"]
        }
    ]
}